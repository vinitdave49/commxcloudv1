<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Commcloud\VoiceBundle\Entity\WWorkspaces" table="w_workspaces">
    <id name="sid" type="integer" column="sid" length="50">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="workspaceSid" type="string" column="workspace_sid" length="50" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="accountSid" type="string" column="account_sid" length="50" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="friendlyName" type="string" column="friendly_name" length="50" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="eventCallbackUrl" type="string" column="event_callback_url" length="500" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="defaultActivitySid" type="string" column="default_activity_sid" length="50" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="dateCreated" type="datetime" column="date_created" nullable="false"/>
    <field name="dateUpdated" type="datetime" column="date_updated" nullable="false"/>
    <field name="defaultActivityName" type="string" column="default_activity_name" length="50" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="timeoutActivitySid" type="string" column="timeout_activity_sid" length="50" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="timeoutActivityName" type="string" column="timeout_activity_name" length="50" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="status" type="integer" column="status" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">1</option>
      </options>
    </field>
  </entity>
</doctrine-mapping>
